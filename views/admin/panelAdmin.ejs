    <script>
          function actualUTC() {
          var myDate = new Date();
          hour = myDate.getUTCHours();
          minutes = myDate.getUTCMinutes();
          seconds = myDate.getUTCSeconds();
          miclock = hour+" : "+minutes+" : "+seconds+" Hora UTC";
          //myDate1=document.getElementById("reloj-id"); //buscar elemento reloj
          //myDate1.innerHTML=mireloj; //incluir hora en elemento
          return miclock;
          }
