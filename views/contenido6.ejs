            $(".btn_porcentage").on("click", function(){
              if($("#barra_especifica6").width() == 0){
                $('#barra_especifica6').css('width', $(this).attr('porcentage')+'%')
              } else {
              const barra = $("#barra_especifica6").width() / $('#barra_especifica6').parent().width() * 100;
              if(barra< $(this).attr('porcentage')){
              $('#barra_especifica6').css('width', $(this).attr('porcentage')+'%')
              }
            }
            $('#barra_especifica6').text($(this).attr('porcentage')+'%');
            })

    </script>
