      <script>
         $(".btn_porcentaje").on("click", function(){
           if($("#barra_especifica1").width() == 0){
             $('#barra_especifica1').css('width', $(this).attr('porcentaje')+'%')
           } else {
           const barra = $("#barra_especifica1").width() / $('#barra_especifica1').parent().width() * 100;
           if(barra< $(this).attr('porcentaje')){
             $('#barra_especifica1').css('width', $(this).attr('porcentaje')+'%')
             }
           }
           $('#barra_especifica1').text($(this).attr('porcentaje')+'%');
           $("#FormAv1").submit(); // Submit the form
         })
         $(".btn_examen").on("click", function(){
           if($("#barra_general").width() == 0){
             $('#barra_general').css('width', $(this).attr('porcentaje')+'%')
           }else{
             const barra = $("#barra_general").width() / $('#barra_general').parent().width() * 100;
             if(barra< $(this).attr('porcentaje')){
               $('#barra_general').css('width', $(this).attr('porcentaje')+'%')
             }
           }
           $('#barra_general').text($(this).attr('porcentaje')+'%');
         })

      </script>
      <script>
        function resultado() {
          var p1, p2, p3, p4, p5, p6, p7, nota;
          //<!-- 1ª pregunta -->
          if (document.getElementById('p11').checked==true) {p1=7}
          else {p1=1}
          //<!-- 2ª pregunta -->
          if (document.getElementById('p23').checked==true) {p2=7}
          else {p2=1}
          //<!-- 3ª pregunta -->
          if (document.getElementById('p32').checked==true) {p3=7}
          else {p3=1}
          //<!-- 4ª pregunta -->
          if (document.getElementById('p43').checked==true) {p4=7}
          else {p4=1}
          //<!-- 4ª pregunta -->
          if (document.getElementById('p51').checked==true) {p5=7}
          else {p5=1}
          //<!-- 4ª pregunta -->
          if (document.getElementById('p63').checked==true) {p6=7}
          else {p6=1}
          //<!-- 4ª pregunta -->
          if (document.getElementById('p71').checked==true) {p7=7}
          else {p7=1}
          nota=(p1+p2+p3+p4+p5+p6+p7)/7;
          var puntodecimal = nota.toFixed(1);
          alert(" Nota: " + puntodecimal);
  
          const btnprueba = document.getElementById('btnprueba');
          btnprueba.disabled = true;
          $(".modal_prueba").modal('dispose');
        }
      </script>

