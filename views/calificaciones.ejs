<!DOCTYPE html>
<html lang="en">

<%- include('templates/header') %>

<body id="page-top">
    <!-- Page Wrapper -->
    <header>
        <!-- NavBar -->
        <nav class="navbar navbar-dark bg-primary">
            <a class="navbar-brand" href="#" style="padding-left:0.5em">
                <img src="img_bicom/planillas/64x64.jpg" width="50" height="50" alt="">
            </a>
            <h3 style="padding-right:33em; color: rgb(255, 255, 255);">Calificaciones</h3>
        </nav>
    </header>
    <div id="wrapper">
        <%- include('templates/siderbars')%>

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                <main class="container" role="main">
                    <div class="bg-light p-5 rounded">
                      <nav aria-label="breadcrumb active">
                        <ol class="breadcrumb">
                          <li class="breadcrumb-item" aria-current="page"><a href="/calificaciones">Vista Calificaciones</a></li>
                        </ol>
                      </nav>
                    </div>
                        <!-- Begin Page Content -->
                        <div class="container-fluid">
                            <!-- DataTales Example -->
                            <div class="card shadow mb-4 border-left-primary text-gray-900 bg-gray-200">
                            <a href="#collapseCardExample3" class="d-block card-header py-3" data-toggle="collapse" role="button" aria-expanded="true" aria-controls="collapseCardExample">
                                <h6 class="m-0 font-weight-bold text-primary">Tabla Calificaciones Videos</h6>
                            </a>
                                <div class="collapse show" id="collapseCardExample3">
                                    <div class="card-body">
                                    <div class="table-responsive">
                                        <table id="" class="display table" width="100%" cellspacing="0">
                                            <thead class="thead-dark">
                                                <tr>
                                                    <th>ID</th>
                                                    <th>ID Acceso</th>
                                                    <th>Nombre</th>
                                                    <th>Apellido</th>
                                                    <th>Email</th>
                                                    <th>Calificacion Videos</th>
                                                    <th>20%</th>
                                                    <th>Curso</th>

                                                </tr>
                                            </thead>
                                            <tfoot class="thead-dark">
                                                <tr>
                                                    <th>ID</th>
                                                    <th>ID Acceso</th>
                                                    <th>Nombre</th>
                                                    <th>Apellido</th>
                                                    <th>Email</th>
                                                    <th>Calificacion Videos</th>
                                                    <th>20%</th>
                                                    <th>Curso</th>
                                                </tr>
                                            </tfoot>
                                            <tbody>
                                                <% vistasCalVideo.forEach((cal_videos)=>{ %>
                                                <tr>
                                                    <td><%= cal_videos.cal_id %></td>
                                                    <td><%= cal_videos.a_id %></td>
                                                    <td><%= cal_videos.p_nombre %></td>
                                                    <td><%= cal_videos.p_apellidos %></td>
                                                    <td><%= cal_videos.p_email %></td>
                                                    <td><%= cal_videos.cal_video %></td>
                                                    <td><%= cal_videos.cal_pond %></td>
                                                    <td><%= cal_videos.cu_descripcion %></td>
                                                </tr>
                                                <% }) %>
                                            </tbody>
                                        </table>
                                        <h5>Ponderacion 20%: <%= promediog1%></h5>
                                    </div>
                                    </div>
                                </div>
                            </div>
                            <!-- DataTales Example -->
                            <div class="card shadow mb-4 border-left-success text-gray-900 bg-gray-200">
                                <a href="#collapseCardExample3" class="d-block card-header py-3" data-toggle="collapse" role="button" aria-expanded="true" aria-controls="collapseCardExample">
                                    <h6 class="m-0 font-weight-bold text-success">Tabla Calificaciones Contenidos</h6>
                                </a>
                                    <div class="collapse show" id="collapseCardExample3">
                                        <div class="card-body">
                                        <div class="table-responsive">
                                            <table id="" class="display table" width="100%" cellspacing="0">
                                                <thead class="thead-dark">
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>ID Acceso</th>
                                                        <th>Nombre</th>
                                                        <th>Apellido</th>
                                                        <th>Email</th>
                                                        <th>Calificacion Contenidos</th>
                                                        <th>80%</th>
                                                        <th>Curso</th>
    
                                                    </tr>
                                                </thead>
                                                <tfoot class="thead-dark">
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>ID Acceso</th>
                                                        <th>Nombre</th>
                                                        <th>Apellido</th>
                                                        <th>Email</th>
                                                        <th>Calificacion Contenidos</th>
                                                        <th>80%</th>
                                                        <th>Curso</th>
                                                    </tr>
                                                </tfoot>
                                                <tbody>
                                                    <% vistaCalContenido.forEach((cal_contenidos)=>{ %>
                                                    <tr>
                                                        <td><%= cal_contenidos.cal_id %></td>
                                                        <td><%= cal_contenidos.a_id %></td>
                                                        <td><%= cal_contenidos.p_nombre %></td>
                                                        <td><%= cal_contenidos.p_apellidos %></td>
                                                        <td><%= cal_contenidos.p_email %></td>
                                                        <td><%= cal_contenidos.cal_prueba %></td>
                                                        <td><%= cal_contenidos.cal_pond %></td>
                                                        <td><%= cal_contenidos.cu_descripcion %></td>
                                                    </tr>
                                                    <% }) %>
                                                </tbody>
                                            </table>
                                            <h5>Ponderacion 80%: <%= promediog2%></h5>
                                        </div>
                                        </div>
                                    </div>
                                </div>
                        <!-- /.container-fluid -->
                        </div>
                </main>

            </div>
            <!-- End of Main Content -->

            <%- include('templates/footer')%>

    </div>
    <!-- End of Content Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

        <!-- Bootstrap core JavaScript-->
        <script src="/assets/vendor/jquery/jquery.min.js"></script>
        <script src="/assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
        <!-- JavaScript Bundle with Popper -->
        <script src="/js/bootstrap.bundle.min.js"></script>
    
        <!-- Core plugin JavaScript-->
        <script src="/assets/vendor/jquery-easing/jquery.easing.min.js"></script>
    
        <!-- Custom scripts for all pages-->
        <script src="/assets/js/sb-admin-2.min.js"></script>

        <!-- Page level plugins -->
        <script src="/assets/vendor/chart.js/Chart.min.js"></script>
    
        <!-- Page level custom scripts -->
        <script src="/assets/js/demo/chart-area-demo.js"></script>
        <script src="/assets/js/demo/chart-pie-demo.js"></script>
        <script src="/assets/js/demo/datatables-demo.js"></script>
        <script src="/assets/vendor/datatables/jquery.dataTables.min.js"></script>
        <script src="/assets/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    
</body>
</html>